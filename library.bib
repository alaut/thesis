	file = {Nakamura et al. - 2017 - Diagnostics, Control and Performance Parameters fo.pdf:F\:\\My Drive\\References\\Zotero\\storage\\Q8IVB8WF\\Nakamura et al. - 2017 - Diagnostics, Control and Performance Parameters fo.pdf:application/pdf},
