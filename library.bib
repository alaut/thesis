@article{kreischer_high_nodate,
	title = {High Power Operation of a 170 {GHz} Gyrotron for {ITER}},
	abstract = {Recent megawatt gyrotron experiments at {MIT} have achieved record powers at 170 {GHz}. Single mode emission with a peak output power of 1.5 {MW} and an efficiency of 35\% has been measured. The experiment is based on a resonant {TE}28 ,8 ,1 cylindrical cavity situated in a 6.7 T magnetic field. Microwaves are generated in the cavity by an 83 {kV} annular electron beam produced by a triode-type magnetron injection gun that is capable of currents up to 50 A. Megawatt power levels with efficiencies between 30-36\% have been measured over a wide range of operating parameters for the {TE}28 ,8 ,1 mode. Similar results were also achieved in the neighboring {TE}27,8 ,1 mode at 166.6 {GHz}, and the {TE}29,g,1 mode at 173.5 {GHz}. The high output power is the result of a carefully designed electron gun with low perpendicular velocity spread (610\%) and a novel cavity with an output iris that is less prone to mode competition. These results are in good agreement with nonlinear multimode simulations.},
	pages = {30},
	author = {Kreischer, K E and Kimura, T and Danly, B G and Temkin, R J},
