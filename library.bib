@article{litvak_development_nodate,
	title = {Development of 1 {MW} Output Power Level Gyrotron for {ITER} {ECRH} System},
	abstract = {The paper presents the approach and results of development of the 170 {GHz}/1 {MW} {ITER} gyrotron in Russia. The main problems of elaborating powerful gyrotrons are discussed. Results of 170 {GHz} gyrotron tests are analyzed and compared with the theory. {ITER} related 110 {GHz} and 140 {GHz} gyrotrons are also investigated. The possibility to create a 170 {GHz}/1 {MW} gyrotron for {ITER} is proved by calculations and experiments.},
