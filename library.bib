@article{wylde_millimetre-wave_1984,
	title = {Millimetre-wave Gaussian beam-mode optics and corrugated feed horns},
	volume = {131},
	issn = {01437097},
	url = {https://digital-library.theiet.org/content/journals/10.1049/ip-h-1.1984.0053},
	doi = {10.1049/ip-h-1.1984.0053},
	abstract = {A Gaussian beam-mode analysis of the operation of a corrugated feed horn is presented. The electric field over the aperture is expanded in terms of a set of Gaussian-Laguerre modes. These modes are solutions to the wave equation, in a paraxial approximation, and propagate from the aperture in a simple and well defined way. The antenna pattern of the horn in any plane in the near or far field is found by summing the contributions from the modes. This process is computationally faster than the standard Fresnel expansion, and provides a new insight into the operation of feed horns. It is particularly appropriate when the horn feeds a quasioptical system. Almost 98\% of the power is carried in the fundamental Gaussian mode. To illustrate the application of the method, an analytical expression for the position of the phase centre is derived and the coupling between two horns is calculated.},
	pages = {258},
	number = {4},
	journaltitle = {{IEE} Proceedings H Microwaves, Optics and Antennas},
	shortjournal = {{IEE} Proc. H Microw. Opt. Antennas {UK}},
	author = {Wylde, R.J.},
	urldate = {2021-01-20},
	date = {1984},
	langid = {english},
	file = {Wylde - 1984 - Millimetre-wave Gaussian beam-mode optics and corr.pdf:F\:\\My Drive\\References\\Zotero\\storage\\64RPP6CW\\Wylde - 1984 - Millimetre-wave Gaussian beam-mode optics and corr.pdf:application/pdf},
}

